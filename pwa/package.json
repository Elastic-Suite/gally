{
  "name": "pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:update": "jest --update-snapshot",
    "eslint": "eslint --fix --max-warnings=0 --ignore-path .gitignore .",
    "eslint:ci": "eslint --max-warnings=0 --ignore-path .gitignore .",
    "stylelint": "stylelint --fix --max-warnings=0 --ignore-path .gitignore '**/*.{css,scss,sass}'",
    "stylelint:ci": "stylelint --max-warnings=0 --ignore-path .gitignore '**/*.{css,scss,sass}'",
    "prettier": "prettier --write --ignore-unknown --ignore-path .gitignore .",
    "prettier:ci": "prettier --check --ignore-unknown --ignore-path .gitignore .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "cd .. && husky install pwa/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "@mui/styles": "^5.6.2",
    "@popperjs/core": "^2.11.5",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/lodash": "^4.14.167",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.3.0",
    "formik": "^2.2.6",
    "husky": "^8.0.1",
    "ionicons": "^6.0.2",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.4.1",
    "i18next-localstorage-backend": "^3.1.3",
    "isomorphic-unfetch": "^3.1.0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^13.0.2",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "next": "^12.0.5",
    "next-i18next": "^11.0.0",
    "next-intl": "^2.7.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@next/eslint-plugin-next": "^12.1.6",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^12.1.5",
    "@types/node": "^17.0.25",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.5.12",
    "jest": "^27.5.1",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "sass": "^1.49.11",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown --ignore-path .gitignore",
    "*.{js,jsx,ts,tsx}": "eslint --fix --max-warnings=0 --ignore-path .gitignore",
    "*.{css,scss,sass}": "stylelint --fix --max-warnings=0 --ignore-path .gitignore"
  }
}
