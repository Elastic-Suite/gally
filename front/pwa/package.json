{
  "name": "pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "NEXT_PUBLIC_LOCAL=true NEXT_PUBLIC_API_URL=http://localhost:3000/ next dev",
    "dev": "next dev",
    "build": "set NODE_OPTIONS=--max-old-space-size=12288 && next build",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests",
    "test:debug": "jest --runInBand",
    "test:update": "jest --update-snapshot",
    "eslint": "eslint --fix --ignore-path .gitignore .",
    "eslint:ci": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --write --ignore-unknown --ignore-path .gitignore .",
    "prettier:ci": "prettier --check --ignore-unknown --ignore-path .gitignore .",
    "typescript": "tsc --noEmit",
    "typescript:ci": "tsc --noEmit",
    "prepare": "cd public && ln -fsT ../../node_modules/@elastic-suite/gally-admin-components/public/locales/ locales && ln -fsT ../../node_modules/@elastic-suite/gally-admin-components/public/images/ images"
  },
  "dependencies": {
    "@elastic-suite/gally-admin-components": "1.2.0-alpha.1",
    "@elastic-suite/gally-admin-shared": "1.2.0-alpha.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.5",
    "@mui/x-date-pickers": "5.0.5",
    "@next/font": "13.x",
    "@reduxjs/toolkit": "1.8.5",
    "autosuggest-highlight": "3.3.4",
    "classnames": "2.3.2",
    "dayjs": "1.11.6",
    "i18next": "22.4.6",
    "i18next-chained-backend": "3.1.0",
    "i18next-http-backend": "1.4.1",
    "i18next-localstorage-backend": "3.1.3",
    "ionicons": "6.0.3",
    "lodash.debounce": "4.0.8",
    "next": "13.x",
    "next-i18next": "13.0.2",
    "notistack": "3.0.0-alpha.11",
    "react": "18.x",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.x",
    "react-redux": "8.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@next/eslint-plugin-next": "12.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/autosuggest-highlight": "3.2.0",
    "@types/jest": "29.0.3",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/parser": "5.37.0",
    "babel-jest": "29.0.3",
    "babel-loader": "8.2.5",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "lint-staged": "13.0.3",
    "postcss": "8.4.16",
    "postcss-scss": "4.0.5",
    "prettier": "2.7.1",
    "require-from-string": "2.0.2",
    "sass": "1.54.9",
    "sass-loader": "13.0.2",
    "tsc-files": "1.1.3",
    "typescript": "4.8.3",
    "webpack": "5.74.0"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown --ignore-path .gitignore",
    "*.{ts,tsx}": "tsc-files --noEmit global.d.ts",
    "*.{js,jsx,ts,tsx}": "eslint --fix --ignore-path .gitignore"
  }
}
